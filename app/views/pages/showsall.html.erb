</head>
<body>


<%= render "pages/navbar" %> 
</div>
	<h1>Suscriptores</h1>
	<div class="table-responsive">
	<table class="table table-bordered">
		<thead>
    		<tr>
    			<th scope="col">#Contacto</th>
     			<th scope="col">Nombre</th>
      			<th scope="col">Email</th>
      			<th scope="col">Tel√©fono</th>
    			<th scope="col">Sindicato</th>
