		<tbody>
		<% @co.each do |c| %>
			<tr>
				<th scope="row"><%= c.id %></th>
				<td><%= c.name %></td>
				<td><%= c.email %></td>
				<td><%= c.phone %></td>
				<td><%= c.sindicato %></td>
				<td><%= c.cargo %></td>
				<td><%= c.created_at.strftime("%e/%b/%Y") %></td>
